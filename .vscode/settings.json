{
    "python.pythonPath": ".venv/bin/python",
    "python.formatting.provider": "yapf",
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "files.associations": {
        "*.podspec": "ruby",
        "python.h": "c",
        "stdio.h": "c",
        "math.h": "c",
        "iostream": "cpp",
        "smc.h": "c",
        "iokitlib.h": "c"
    }
}